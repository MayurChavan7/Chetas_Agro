{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*\\.jpg|.*\\.png|.*\\.jpeg|.*\\.gif)", "dest": "/$1" }, 
    { "src": "/assets/(.*)", "dest": "/assets/$1" },                        
    { "src": "/(.*)", "dest": "/index.html" }                               
  ]
}
